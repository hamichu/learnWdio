{
  "name": "learnwdio",
  "version": "1.0.0",
  "description": "Окружение для обучения http://webdriver.io",
  "main": "wdio.conf.js",
  "scripts": {
    "selenium:install": "selenium-standalone install",
    "test": "node_modules/.bin/wdio wdio.conf.js",
    "repl": "./node_modules/.bin/wdio wdio.conf.js --spec ./repl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurseryjs/learnWdio.git"
  },
  "author": "Eddort",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nurseryjs/learnWdio/issues"
  },
  "babel": {
    "retainLines": true,
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "shippedProposals": true,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "syntax-object-rest-spread"
    ]
  },
  "homepage": "https://github.com/nurseryjs/learnWdio#readme",
  "dependencies": {
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-webdriverio": "^0.4.3",
    "cpy-cli": "^1.0.1",
    "dotenv": "5.0.1",
    "faker": "^4.1.0",
    "jimp": "^0.2.28",
    "lodash.ismatch": "^4.4.0",
    "lodash.random": "^3.2.0",
    "lodash.sample": "^4.2.1",
    "mochawesome-report-generator": "^2.3.2",
    "require-glob": "3.2.0",
    "wdio-docker-service": "1.1.7",
    "wdio-json-reporter": "0.3.1",
    "wdio-mocha-framework": "0.5.13",
    "wdio-mochawesome-reporter": "1.3.0",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "0.1.4",
    "webdriverio": "4.12.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsdoc": "3.1.3",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-webdriverio": "1.0.1",
    "husky": "0.14.3",
    "jsdoc": "3.5.5",
    "jsdoc-api": "4.0.3",
    "jsdoc-to-markdown": "4.0.1",
    "lint-staged": "6.0.0",
    "minami": "1.2.3",
    "rimraf": "^2.6.2"
  }
}
